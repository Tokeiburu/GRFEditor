    <Style x:Key="TkTreeViewItemStyle" TargetType="{x:Type WPF:TkTreeViewItem}">
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Border Name="_tviHeader" Background="Transparent" Margin="-2 0 0 0">
                        <Border Name="_tviHeaderBorder" CornerRadius="2" BorderThickness="1">
                            <StackPanel Orientation="Horizontal" Margin="3 0 1 0">
                                <Image Name="_image" Height="16" Width="16" Stretch="Uniform" Source="../../Resources/folderClosed.png" RenderOptions.BitmapScalingMode="HighQuality" />
                                <Grid>
                                    <StackPanel Orientation="Horizontal" Margin="0">
                                        <TextBlock Text="{Binding Path=HeaderText, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Name="_tbDisplay" Margin="4 0 0 0" VerticalAlignment="Center" Foreground="{DynamicResource UITreeViewDefaultForegroundBrush}" />
                                        <TextBlock Text="{Binding Path=TranslatedText, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Name="_tbDisplayVirtual" FontStyle="Italic" Foreground="Gray" Margin="0 0 4 0" VerticalAlignment="Center" />
                                    </StackPanel>
                                    <Grid Margin="2 0 0 0" Name="_gridEdit" Visibility="Collapsed">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto" />
                                            <ColumnDefinition Width="30" />
                                        </Grid.ColumnDefinitions>
                                        <TextBox SelectionBrush="#55A2FF" MaxHeight="{Binding Path=ActualHeight, ElementName=_tbDisplay}" BorderThickness="0" Margin="0" Grid.ColumnSpan="2" Padding="0" VerticalAlignment="Center" Foreground="Transparent" Name="_tbEdit" Visibility="Collapsed" />
                                        <TextBox SelectionBrush="#55A2FF" MaxHeight="{Binding Path=ActualHeight, ElementName=_tbDisplay}" BorderThickness="0" Margin="0" Padding="0" VerticalAlignment="Center" Foreground="{DynamicResource UITreeViewSelectedForegroundBrush}" Name="_tbOverlay" Visibility="Collapsed" 
                                                 IsHitTestVisible="False" Background="Transparent"
                                                 Text="{Binding Path=Text, ElementName=_tbEdit}"/>
                                        
                                    </Grid>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </Border>
                    <DataTemplate.Triggers>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=IsExpanded, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True" />
                                <Condition Binding="{Binding Path=CanExpand, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="_image" Property="Source" Value="../../Resources/folderOpened.png" />
                        </MultiDataTrigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="_tviHeaderBorder" Property="Background" Value="{DynamicResource UITreeViewMouseOverBackgroundBrush}" />
                            <Setter TargetName="_tviHeaderBorder" Property="BorderBrush" Value="{DynamicResource UITreeViewMouseOverBorderBrush}" />
                            <Setter TargetName="_tbDisplay" Property="Foreground" Value="{DynamicResource UITreeViewMouseOverForegroundBrush}" />
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=IsSelected, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True">
                            <Setter TargetName="_tviHeaderBorder" Property="Background" Value="{DynamicResource UITreeViewSelectedBackgroundBrush}" />
                            <Setter TargetName="_tviHeaderBorder" Property="BorderBrush" Value="{DynamicResource UITreeViewSelectedBorderBrush}" />
                            <Setter TargetName="_tbDisplay" Property="Foreground" Value="{DynamicResource UITreeViewSelectedForegroundBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=IsDragEnter, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True">
                            <Setter TargetName="_tviHeaderBorder" Property="Background" Value="{DynamicResource UITreeViewDragEnterBackgroundBrush}" />
                            <Setter TargetName="_tviHeaderBorder" Property="BorderBrush" Value="{DynamicResource UITreeViewDragEnterBorderBrush}" />
                            <Setter TargetName="_tbDisplay" Property="Foreground" Value="{DynamicResource UITreeViewMouseOverForegroundBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=IsUnableItem, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True">
                            <Setter TargetName="_tviHeaderBorder" Property="Background" Value="{DynamicResource UITreeViewUnableBackgroundBrush}" />
                            <Setter TargetName="_tviHeaderBorder" Property="BorderBrush" Value="{DynamicResource UITreeViewUnableBorderBrush}" />
                            <Setter TargetName="_tbDisplay" Property="Foreground" Value="{DynamicResource UITreeViewMouseOverForegroundBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=IsEditMode, RelativeSource={RelativeSource AncestorType={x:Type WPF:TkTreeViewItem}}}" Value="True">
                            <Setter TargetName="_tbDisplay" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="_tbDisplayVirtual" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="_tbEdit" Property="Visibility" Value="Visible" />
                            <Setter TargetName="_tbOverlay" Property="Visibility" Value="Visible" />
                            <Setter TargetName="_gridEdit" Property="Visibility" Value="Visible" />
                            <Setter TargetName="_tviHeaderBorder" Property="Background" Value="Transparent" />
                            <Setter TargetName="_tviHeaderBorder" Property="BorderBrush" Value="{DynamicResource UITreeViewSelectedForegroundBrush}" />
                        </DataTrigger>
                    </DataTemplate.Triggers>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

